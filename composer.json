{
    "name": "daviddlv/clarifai-bundle",
    "type": "symfony-bundle",
    "description": "Image recognition for your Symfony project using Clarifai",
    "keywords": [
        "image",
        "recognition",
        "clarifai",
        "symfony",
        "bundle"
    ],
    "authors": [
        {
            "name": "David Delevoye",
            "email": "daviddelevoye@gmail.com"
        },
        {
            "name": "Vincent CHALAMON",
            "email": "vincent@les-tilleuls.coop"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ClarifaiBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/dependency-injection": "~2.7|~3.0",
        "symfony/config": "~2.8|~3.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "behat/symfony2-extension": "^2.1",
        "symfony/browser-kit": "^3.2",
        "phpunit/phpunit": "^5.7",
        "symfony/framework-bundle": "^2.8|^3.0",
        "gorghoa/scenariostate-behat-extension": "^1.0"
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
